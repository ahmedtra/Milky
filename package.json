{
  "name": "milky-diet-assistant",
  "version": "1.0.0",
  "description": "AI-powered diet assistant with Gemini AI and Telegram notifications",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.0",
    "@google/generative-ai": "^0.2.1",
    "@zilliz/milvus2-sdk-node": "^2.6.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "diet",
    "ai",
    "gemini",
    "telegram",
    "meal-planning"
  ],
  "author": "Your Name",
  "license": "MIT"
}
